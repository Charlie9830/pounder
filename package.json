{
  "name": "pounder",
  "productName": "Handball",
  "author": "Charlie Hall",
  "description": "Widget based ToDo app",
  "version": "2.0.3",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "testDev": "electron .",
    "testProd": "electron . --noDevServer",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "devWeb": "webpack-dev-server --hot --host 0.0.0.0 --public 172.16.21.249:8080 --config=./webpack.dev.web.config.js",
    "build": "webpack --config webpack.build.config.js",
    "packageWeb": "webpack --config webpack.build.web.config.js",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "electron-packager ./ --overwrite --asar --icon=./src/assets/icons/icon.ico --platform=win32 --arch=x64 --out=./builds",
    "buildInstaller": "node build-windows-installer.js",
    "packagemac": "electron-packager ./ --asar --platform=darwin --overwrite --arch=x64 --icon=./src/assets/icons/icon.icns --out=./builds",
    "builddmg": "node build-darwin-dmg.js ",
    "electronBuildWin": "electron-builder --win --x64",
    "electronBuildMac": "electron-builder --mac --x64",
    "winRelease": "build --publish always -c ./electron-builder-configs/win-publish-to-github.yaml",
    "macRelease": "build --publish always -c ./electron-builder-configs/mac-publish-to-github.yaml",
    "testWinRelease": "build --publish always -c ./electron-builder-configs/win-publish-to-test.yaml",
    "testMacRelease": "CSC_IDENTITY_AUTO_DISCOVERY=false build -c ./electron-builder-configs/mac-publish-to-test.yaml",
    "startTestReleaseServer": "http-server ./test-release-server -p 3001"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Charlie9830/pounder.git"
  },
  "devDependencies": {
    "app-builder-lib": "^20.27.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "css-loader": "^0.28.11",
    "devtron": "^1.4.0",
    "electron": "2.0.6",
    "electron-builder": "^20.26.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^12.1.0",
    "electron-react-devtools": "*",
    "electron-winstaller": "^2.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.30.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "style-loader": "^0.20.3",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "dexie": "^2.0.4",
    "electron-log": "^2.2.16",
    "electron-updater": "^3.0.3",
    "fs-jetpack": "^1.3.0",
    "hammerjs": "^2.0.8",
    "later": "^1.2.0",
    "moment": "^2.22.2",
    "mousetrap": "^1.6.2",
    "open": "0.0.5",
    "react-autosize-textarea": "^3.0.3",
    "react-color": "^2.14.1",
    "react-day-picker": "^6.2.1",
    "react-grid-layout": "^0.16.6",
    "react-hot-loader": "^4.3.1",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "react-resizable": "^1.7.5",
    "react-sidebar": "^2.3.2",
    "react-sizeme": "^2.5.2",
    "react-tooltip": "^3.6.1",
    "react-transition-group": "^2.4.0",
    "redux": "^4.0.0",
    "sanitize-filename": "^1.6.1",
    "typeface-open-sans": "0.0.54",
    "yui-lint": "^0.2.0"
  }
}
